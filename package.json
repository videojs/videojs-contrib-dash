{
  "name": "videojs-contrib-dash",
  "description": "A Video.js source-handler providing MPEG-DASH playback.",
  "main": "dist/videojs-dash.cjs.js",
  "module": "dist/videojs-dash.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/videojs/videojs-contrib-dash.git"
  },
  "version": "2.9.0",
  "author": "Brightcove, Inc",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all build:js:*",
    "build:js": "npm-run-all -p build:js:dashjs build:js:shaka",
    "build:js:dashjs": "npm-run-all build:js:rollup build:js:bannerize build:js:uglify",
    "build:js:shaka": "npm-run-all build:js:rollup-shaka build:js:bannerize-shaka build:js:uglify-shaka",
    "build:js:bannerize": "bannerize dist/videojs-dash.js --banner=scripts/banner.ejs",
    "build:js:bannerize-shaka": "bannerize dist/videojs-shaka.js --banner=scripts/banner.ejs",
    "build:js:rollup": "rollup -c scripts/umd.config.js && rollup -c scripts/modules.config.js",
    "build:js:rollup-shaka": "rollup -c scripts/umd.config.js --environment SHAKA && rollup -c scripts/modules.config.js --environment SHAKA",
    "build:js:uglify": "uglifyjs dist/videojs-dash.js --support-ie8 --comments -m -c -o dist/videojs-dash.min.js",
    "build:js:uglify-shaka": "uglifyjs dist/videojs-shaka.js --support-ie8 --comments -m -c -o dist/videojs-shaka.min.js",
    "build:test:browserify": "browserify test/build.test.js -o dist-test/browserify.test.js",
    "build:test:webpack": "webpack test/build.test.js dist-test/webpack.test.js",
    "clean": "npm-run-all -p clean:*",
    "clean:build": "rm -rf dist dist-test && mkdir -p dist dist-test",
    "lint": "jshint src/**/*.js",
    "pretest": "npm-run-all lint build build:test:*",
    "test": "karma start test/karma.config.js && karma start test/shaka.karma.config.js",
    "preversion": "npm test",
    "version": "npm run build",
    "prepublish": "npm run build"
  },
  "keywords": [
    "videojs",
    "dashjs",
    "video.js",
    "dash.js",
    "widevine",
    "playready",
    "dash",
    "MPEG-DASH"
  ],
  "files": [
    "CONTRIBUTING.md",
    "dist/",
    "docs/",
    "index.html",
    "scripts/",
    "src/",
    "test/"
  ],
  "dependencies": {
    "dashjs": "^2.2.0",
    "global": "^4.3.0",
    "shaka-player": "2.0.7",
    "video.js": "^5.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es3": "^1.0.1",
    "bannerize": "^1.0.0",
    "browserify": "^14.3.0",
    "jshint": "^2.9.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-qunit": "^1.2.1",
    "karma-spec-reporter": "0.0.31",
    "npm-run-all": "^1.2.0",
    "qunitjs": "^2.0.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "uglify-js": "^2.5.0",
    "webpack": "^1.13.2"
  }
}
